<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.email %></td>
        <td><%= link_to 'Show', user_registration_path %></td>
        <td><%= link_to 'Edit', edit_user_registration_path(user) %></td>
      
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New user', new_user_registration_path %>